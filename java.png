                    $(this).parent().parent().after('<div class="hata">Yorum alanını doldurunuz.</div>');
                }else if($(this).parent().find('input[type=radio]:checked').val() == undefined){
                    $(this).parent().parent().after('<div class="hata">Lütfen puanlama yapınız.</div>');
                }else{

                    e.preventDefault();
                    var form = $("#comment_form");
                    var formUrl = form.attr('action');
                    var formId = form.attr('id');
                    var formData = form.serialize();

                    $.ajax({
                        url: formUrl,
                        type: "post",
                        data : formData,
                        success: function (result){
                            $('#yorumlar .inputarea').remove();
                            $('.returnResponse').after('<div class="basari">Form başarıyla iletilmiştir. En kısa sürede tarafınıza geri dönüş yapılacaktır.</div>');
                        },
                        error: function (result){
                            var arr = JSON.parse(result.responseText);
                            $("div#error-" + formId + ">ul:first").html("");
                            $.each(arr, function (k, v) {
                                $("div#error-" + formId + ">ul:first").append('<li>' + v + "</li>").closest("div").show();
                            });
                        }
                    });


                    // $('#yorumlar .inputarea').after('<div class="basari">Yorumunuz başarılı bir şekilde alınmıştır.</div>');
                    // $('#yorumlar .inputarea').remove();
                }
            });

            $("form.sendForm").submit(function(e){
                e.preventDefault();

                var formData = $(this).serialize();

                $('input[id="commentSendBtn"]').prop('disabled',true);

                $.ajax({
                    method: "POST",
                    url: $(this).attr('action'),
                    data: formData,
                    cache: false
                }).error(function(msg,error,statusCode){
                    var message = ' <div class="alert alert-danger"><ul>';
                    if(msg.status == 422){
                        var data = JSON.parse(msg.responseText);
                        for(var i in data){
                            message += '<li>'+data[i]+'</li>';
                        }
                    }else{
                        message += '<li>Kayıt hatası oluştu, lütfen daha sonra tekrar deneyiniz.</li>';
                    }
                    message += '</ul></div>';
                    $('#alert-modal-body').html(message);

                    $('input[id="commentSendBtn"]').prop('disabled',false);

                }).done(function( data ) {
                    $("form.sendForm")[0].reset();
                    var message = ' <div class="alert alert-success"><ul><li>Yorumunuz gönderildi, yönetici onayının ardından yayınlanacaktır.</li></ul></div>';
                    $('#alert-modal-body').html(message);

                    $('input[id="commentSendBtn"]').prop('disabled',false);
                });

            });

        });
    </script>
